{
  "name": "TradingPortal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "startrs": "npm start -- --reset-cache",
    "checkdeps": "ncu --packageFile package.json",
    "i5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "i6": "react-native run-ios --simulator=\"iPhone 6\"",
    "i6s": "react-native run-ios --simulator=\"iPhone 6s\"",
    "xsm": "react-native run-ios --simulator=\"iPhone XS Max\"",
    "i6p": "react-native run-ios --simulator=\"iPhone 6 Plus\"",
    "x": "react-native run-ios --simulator=\"iPhone X\"",
    "di6p": "react-native run-ios --device \"admin’s iPhone\"",
    "di6s": "react-native run-ios --device \"Home’s iPhone\"",
    "di5": "react-native run-ios --device \"Tan's iPhone\"",
    "ls-ios": "instruments -s devices",
    "a": "react-native run-android",
    "ar": "react-native run-android --variant=\"release\"",
    "shake": "adb shell am broadcast -a react.native.RELOAD",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "appcenter-ios": "cd ios && fastlane appcenter && cd ..",
    "appcenter-a": "cd android && fastlane appcenter && cd ..",
    "release": "cd ios && fastlane release && cd ..",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run git-hook && npm run fixcode",
    "fixcode": "standard --fix",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "lint": "standard --verbose | snazzy",
    "git-hook": "npm run lint -s"
  },
  "dependencies": {
    "add": "^2.0.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "native-echarts": "^0.5.0",
    "querystringify": "^2.1.1",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-drawer-layout-polyfill": "^2.0.0",
    "react-native-elements": "^1.1.0",
    "react-native-f2chart": "^1.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-svg": "^6.5.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.4",
    "reselect": "^4.0.0",
    "socket.io": "^2.2.0",
    "victory-native": "^30.0.0",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint-plugin-flowtype": "^3.2.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "^0.49.2",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.2",
    "react-test-renderer": "16.6.3",
    "schedule": "0.5.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/out/",
      "/node_modules/",
      "/scripts/"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element",
      "DatePickerAndroid",
      "alert",
      "string",
      "number"
    ]
  }
}
